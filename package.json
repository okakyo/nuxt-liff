    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/composition-api": "^1.0.0-beta.1",
    "nuxt": "^2.13.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config": "^3.0.0",
